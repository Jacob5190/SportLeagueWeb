<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pojo.CalendarEvent">
    <insert id="insertCalendarEvent" parameterType="pojo.CalendarEvent">
        insert into calendar_event (startDate, title)
        values (#{startDate}, #{title});
    </insert>
    <select id="selectCalendarEvent" resultType="pojo.CalendarEvent">
        select id, startDate, title from calendar_event;
    </select>
    <select id="selectCalendarEventId" resultType="int">
        select id from calendar_event;
    </select>
    <delete id="deleteCalendarEvent">
        delete from calendar_event where id = #{id};
    </delete>
</mapper>
<mapper namespace="pojo.User">
    <select id="selectUsr">
        select * from
    </select>
</mapper>
